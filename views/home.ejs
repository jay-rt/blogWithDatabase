<%- include("partials/header") -%>

<h1><%= homeContent.title %></h1>
<p><%= homeContent.content %></p>

<% postedContent.forEach(post => { %>
<h2><%= post.title %></h2>
<p>
  <%= post.content.substring(0, 100) + " ..." %>
  <a href="/posts/<%= post.slug %>">Read More</a>
</p>
<% }) %> <%- include("partials/footer") -%>
